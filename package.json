  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.16.3",
    "chai": "^4.0.0-canary.1",
    "eslint": "^3.8.0",
